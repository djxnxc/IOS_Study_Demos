#import "RoundLabel.h"

@implementation RoundLabel

- (id)initWithFrame:(CGRect)frame
{
    self = [super initWithFrame:frame];
    if (self) {
        self.layer.cornerRadius = 10;
    }
    return self;
}
- (void)drawRect:(CGRect)rect {
    self.layer.cornerRadius = 10;
}

@end
